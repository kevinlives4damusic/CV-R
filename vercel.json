{
  "version": 2,
  "buildCommand": "npm install -D tailwindcss postcss autoprefixer && npm run build",
  "devCommand": "npm run dev",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "installCommand": "npm install"
}
